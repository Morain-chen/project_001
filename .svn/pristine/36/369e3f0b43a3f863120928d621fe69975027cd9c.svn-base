package com.xx.entity;

public class Orderr {
	public Integer getOid() {
		return oid;
	}
	public void setOid(Integer oid) {
		this.oid = oid;
	}
	public Integer getOnumber() {
		return onumber;
	}
	public void setOnumber(Integer onumber) {
		this.onumber = onumber;
	}
	public Userdata getUid() {
		return uid;
	}
	public void setUid(Userdata uid) {
		this.uid = uid;
	}
	public Integer getOrtotal() {
		return ortotal;
	}
	public void setOrtotal(Integer ortotal) {
		this.ortotal = ortotal;
	}
	public String getOrtime() {
		return ortime;
	}
	public void setOrtime(String ortime) {
		this.ortime = ortime;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
	//编号
	private Integer oid;
	//订单编号
	private Integer onumber;
	//用户编号
	private Userdata uid;
	//总价
	private Integer ortotal;
	//时间
	private String ortime;
	//备注
	private String remark;
	//状态
	private String state;
}

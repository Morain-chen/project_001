package com.xingxue.kkxy.service;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import com.xingxue.kkxy.dao.UserDao;
import com.xingxue.kkxy.entity.Users;

@Service
public class UsersServiceImpl implements UsersService {
	@Resource
	private UserDao us;

	public int addUsers(Users user) {
		// TODO Auto-generated method stub
		return us.addUser(user);
	}

	@Override
	public Users alterBeforSelectUser(int id) {
		// TODO Auto-generated method stub
		return us.alterBeforSelectUser(id);
	}

	@Override
	public int alterUser(Users users) {
		// TODO Auto-generated method stub
		return us.alterUser(users);
	}

}
